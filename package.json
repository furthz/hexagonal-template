{
  "name": "template",
  "version": "1.0.0",
  "description": "plantilla proyecto ",
  "license": "ISC",
  "author": {
    "name": "Raúl Talledo Bermúdez",
    "email": "raul.talledo@soapros.pe"
  },
  "main": "dist/src/main.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:tsc",
    "build:clean": "rimraf dist; exit 0",
    "build:tsc": "tsc -p tsconfig.build.json",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --color",
    "check:markdown": "markdownlint '**/*.md'",
    "check:packagejson": "npmPkgJsonLint .",
    "check:staged": "lint-staged",
    "check:types": "tsc --pretty --noEmit",
    "commit": "cz",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "npm run check:lint -- --fix",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "reset-hard": "git clean -dfx && git reset --hard && npm install",
    "start": "node dist/src/main.js",
    "start:dev": "tsnd --inspect=0.0.0.0:9229 --respawn src/main.ts",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test jest --coverage",
    "version": "standard-version -t"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-hexagonal-architecture": "^1.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-yml": "^1.10.0",
    "get-tsconfig": "^4.7.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "npm-check-updates": "^16.14.6",
    "npm-package-json-lint": "^7.1.0",
    "npm-package-json-lint-config-default": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "yaml-eslint-parser": "^1.2.2"
  }
}
